
   <%= form_for(@user, html: {multipart: true})  do |f| %>
       <%= hidden_field '_method', value:'patch' %>

       <%= f.label 'username'%> 
       <%= f.text_field 'username', placeholder:'Your username here' %></br></br>

       <%= f.label 'email'%>
       <%= f.email_field 'email', placeholder:'example@example.com' %> </br></br> 

       <%= f.label 'password'%>
       <%= f.password_field 'password'%> </br></br>

       <%= f.submit 'Submit'%> 
    <%end%>